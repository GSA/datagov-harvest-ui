{
	"name": "datagov-harvest-ui",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev:vite": "vite dev",
		"dev": "concurrently --kill-others-on-fail \"npm run json-serve\" \"npm run dev:vite\"",
		"build": "vite build",
		"json-serve": "json-server db.json",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"concurrently": "^8.2.2",
		"json-server": "^1.0.0-alpha.23",
		"rollup-plugin-copy": "^3.5.0",
		"sass": "^1.71.1",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.7",
		"typescript": "^5.4.2",
		"vite": "^5.1.6"
	},
	"type": "module",
	"dependencies": {
		"@uswds/uswds": "3.8.0",
		"chart.js": "^4.4.2",
		"svelte-chartjs": "^3.1.5"
	}
}
